{
  "name": "ModernDeck",
  "version": "0.0.7",
  "description": "ModernDeck",
  "main": "main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "electron ."
  },
  "build": {
    "appId": "com.dangeredwolf.ModernDeck",
    "win": {
      "legalTrademarks": "Copyright 2014-2019 dangered wolf, released under the MIT licence",
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "squirrelWindows": {
      "publish": [
        "github"
      ]
    }
  },
  "repository": "https://github.com/dangeredwolf/ModernDeck",
  "keywords": [
    "TweetDeck",
    "ModernDeck",
    "Modern",
    "Tweet",
    "Deck",
    "Material",
    "Design"
  ],
  "author": "dangeredwolf",
  "license": "MIT",
  "dependencies": {
    "electron-packager": "^13.0.0",
    "electron-serve": "^0.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.0.0",
    "electron-log":"^3.0.5",
    "electron-save-file":"^1.0.2",
    "electron-store": "^1.3.0",
    "jquery": "^3.3.1",
    "materialize-css": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0"
  },
  "files": [
    "**/ModernDeck",
    "*.js",
    "*.css",
    "*.png",
    "*.woff2",
    "*.woff",
    "*.json",
    "*.ico"
  ]
}
